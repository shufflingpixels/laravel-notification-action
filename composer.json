{
    "name": "shufflingpixels/laravel-notification-action",
    "description": "Adds actionable endpoints for Laravel database notifications.",
    "type": "library",
    "license": "AGPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "ShufflingPixels\\NotificationAction\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShufflingPixels\\NotificationAction\\Providers\\NotificationActionServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Henrik Hautakoski",
            "email": "henrik@shufflingpixels.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/notifications": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5|^11.0|^12.0",
        "orchestra/testbench": "^7.0|^8.0|^10.0",
        "laravel/pint": "^1.26"
    },
    "autoload-dev": {
        "psr-4": {
            "ShufflingPixels\\NotificationAction\\Tests\\": "tests/"
        }
    }
}
